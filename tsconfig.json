{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@fossapp/core": [
        "./packages/core/src/index.ts"
      ],
      "@fossapp/core/*": [
        "./packages/core/src/*"
      ],
      "@fossapp/products": [
        "./packages/products/src/index.ts"
      ],
      "@fossapp/products/*": [
        "./packages/products/src/*"
      ],
      "@fossapp/tiles": [
        "./packages/tiles/src/index.ts"
      ],
      "@fossapp/tiles/*": [
        "./packages/tiles/src/*"
      ],
      "@fossapp/projects": [
        "./packages/projects/src/index.ts"
      ],
      "@fossapp/projects/*": [
        "./packages/projects/src/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
